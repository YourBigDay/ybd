	function readBrideFile(input) {
 			if (input.files && input.files[0]) {
	            var reader = new FileReader();
	            
	            reader.onload = function (e) {
					var classes = document.getElementById('bride-pic').classList;
					if ( !classes.contains('ready')) {
						classes.add('ready');
					}
									
					bridePic.bind({
	            		url: e.target.result
	            	});
	            }
	            reader.readAsDataURL(input.files[0]);
	        }
	        else {
		        alert("Sorry - you're browser doesn't support the FileReader API");
		    }
		}
		
		
	function readGroomFile(input) {
